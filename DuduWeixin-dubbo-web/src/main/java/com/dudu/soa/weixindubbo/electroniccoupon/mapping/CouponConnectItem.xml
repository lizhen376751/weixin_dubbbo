<?xml version="1.0" encoding = "UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dudu.soa.weixindubbo.electroniccoupon.mapper.CouponConnectItem">

    <insert id="addConnectWithItem" parameterType="com.dudu.soa.weixindubbo.electroniccoupon.module.CouponConnect"
            useGeneratedKeys="true" keyProperty="id" flushCache="true">
        INSERT Coupon_Item (
            serviceCode,
            couponId,
            shopCode,
            serviceFlag,
            shopCodeLM
        )
        VALUES
            (
                #{serviceCode},
                #{couponId},
                #{shopCode},
                #{serviceFlag},
                #{shopCodeLM}
            )
    </insert>
</mapper>